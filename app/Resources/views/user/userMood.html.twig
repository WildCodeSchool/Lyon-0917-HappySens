{% if user.mood != 0 or user.mood != null %}
    {% for i in 1..user.mood %}
        <i class="material-icons amber-text text-darken-2">star</i>
    {% endfor %}
    {% if user.mood != 5 %}
        {% for i in 5..(user.mood+ 1) %}
            <i class="material-icons  amber-text text-darken-2">star_border</i>
        {% endfor %}
    {% endif %}
{% else %}
    {% for i in 1..5 %}
        <i class="material-icons amber-text text-darken-2">star_border</i>
    {% endfor %}
{% endif %}